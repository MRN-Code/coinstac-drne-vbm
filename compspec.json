{
  "meta": {
    "name": "Decentralized Regression for VBM",
    "id": "regression-vbm",
    "version": "v1.0.0",
    "repository": "https:\/\/github.com\/hvgazula\/coinstac-regression-vbm",
    "description": "Decentralized regression (normal equation) for VBM"
  },
  "computation": {
    "type": "docker",
    "dockerImage": "regression-vbm",
    "command": [
      "python",
      "\/computation\/scripts\/local.py"
    ],
    "remote": {
      "type": "docker",
      "dockerImage": "regression-vbm",
      "command": [
        "python",
        "\/computation\/scripts\/remote.py"
      ]
    },
    "input": {
      "covariates": {
        "type": "array"
      },
      "dependents": {
        "type": "array"
      },
      "lambda": {
        "type": "number"
      }
    },
    "output": {
      "Beta": {
        "type": "array"
      }
    }
  }
}
